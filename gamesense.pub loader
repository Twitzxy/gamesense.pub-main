            armor = {enabled = true},
            distance = {enabled = true},
            tool = {enabled = true}
        },
        BarLayout = {
            health = {
                enabled = true,
                color_full = MainColor,
                color_empty = Color3.fromRGB(255, 255, 255)
            },
            armor = {
                enabled = true,
                color_full = MainColor,
                color_empty = MainColor
            }
        },
